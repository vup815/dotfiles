gfeatm() {
        branch="$(git rev-parse --abbrev-ref HEAD)" 
            git commit -m "feature($branch): $1"
}

gfixm() {
        branch="$(git rev-parse --abbrev-ref HEAD)" 
            git commit -m "fix($branch): $1"
}

mysql_sit() {
    mysql -u jkopay-appsvc -p'wqe8vgw3efv7xda_BDT' -h mysql-001-m-01.jkos.sit $1
}

sit_mssql() {
        sqlcmd -S sit-mssql.jkopay.app -d JKOPay_SIT -U appsvc -Q "$1"
}

maven_skip_test() {
    mvn clean $1 -Dmaven.test.skip=true
}

prod_mssql() {
        sqlcmd -S $1 -d $2 -U $3 -K ReadOnly -Q "$4" 
} 

