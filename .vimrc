set nocompatible              " be iMproved, required
filetype off                  " required

set shell=/bin/zsh

syntax on
set clipboard=unnamed 
set autoindent
set number
set relativenumber
set cursorline
set incsearch
set ignorecase
set ts=4
set expandtab
set nohlsearch

nnoremap j gj
nnoremap k gk

imap jk <esc>

